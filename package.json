{
  "name": "react-redux-donderstarter",
  "version": "1.0.0",
  "description": "react, redux, react-router boilerplate",
  "main": "dist/index.js",
  "scripts": {
    "dev": "webpack-dev-server"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Cloudoki/react-redux-donderstarter.git"
  },
  "keywords": [
    "react",
    "redux",
    "react-router",
    "boilerplate"
  ],
  "contributors": [
    "muffles <molefas@gmail.com>",
    "mstrk <mstrk512@gmail.com> (http://www.meetbruno.eu/)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Cloudoki/react-redux-donderstarter/issues"
  },
  "homepage": "https://github.com/Cloudoki/react-redux-donderstarter.git#readme",
  "dependencies": {
    "history": "4.6.1",
    "prop-types": "15.5.10",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "react-redux": "5.0.5",
    "react-router": "4.1.1",
    "react-router-redux": "next",
    "redux": "3.6.0",
    "redux-logger": "3.0.6",
    "redux-saga": "0.15.3",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "autoprefixer": "7.1.1",
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.0.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "babili-webpack-plugin": "0.1.1",
    "css-hot-loader": "1.1.1",
    "css-loader": "0.28.4",
    "eslint": "3.19.0",
    "eslint-config-standard": "10.2.1",
    "eslint-config-standard-react": "5.0.0",
    "eslint-import-resolver-webpack": "0.8.1",
    "eslint-plugin-import": "2.3.0",
    "eslint-plugin-node": "4.2.2",
    "eslint-plugin-promise": "3.5.0",
    "eslint-plugin-react": "7.0.1",
    "eslint-plugin-standard": "3.0.1",
    "extract-text-webpack-plugin": "2.1.0",
    "html-webpack-plugin": "2.28.0",
    "node-sass": "4.5.3",
    "postcss-loader": "2.0.5",
    "react-hot-loader": "next",
    "sass-loader": "6.0.5",
    "style-loader": "0.18.1",
    "webpack": "2.6.1",
    "webpack-dev-server": "2.4.5"
  },
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "es2015": {
            "loose": true,
            "modules": false
          }
        }
      ],
      "react",
      "stage-2"
    ],
    "plugins": [
      "react-hot-loader/babel"
    ]
  },
  "eslintConfig": {
    "extends": [
      "standard",
      "standard-react"
    ],
    "parser": "babel-eslint",
    "env": {
      "browser": true
    },
    "rules": {
      "prefer-const": 2,
      "no-var": 2,
      "prefer-arrow-callback": 2,
      "arrow-body-style": 2,
      "react/no-unused-prop-types": 0,
      "import/no-unresolved": 2
    },
    "settings": {
      "import/resolver": "webpack"
    }
  }
}
