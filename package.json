    "url-loader": "1.0.1",
    "webpack": "4.5.0",
    "webpack-cli": "2.0.14",
    "webpack-dev-server": "3.1.3",
    "webpack-merge": "4.1.2",
    "webpack-visualizer-plugin": "0.1.11"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
