{
  "name": "react-redux-donderstarter",
  "version": "1.0.0",
  "description": "react, redux, react-router boilerplate",
  "main": "dist/index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server",
    "mock-api": "json-server --watch __mocks__/db.json --port 9004",
    "prebuild": "yarn test && rimraf dist",
    "build": "cross-env NODE_ENV=production webpack --progress",
    "test": "cross-env NODE_ENV=test jest --c ./jest.json --coverage",
    "precommit": "yarn test",
    "commit": "git-cz",
    "prestart": "yarn build",
    "start": "http-server-spa ./dist index.html 8080",
    "open-cov": "open coverage/lcov-report/index.html",
    "prerelease": "git fetch --tags",
    "release": "standard-version",
    "generate": "node ./internals/generate"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Cloudoki/react-redux-donderstarter.git"
  },
  "keywords": [
    "react",
    "redux",
    "react-router",
    "boilerplate"
  ],
  "contributors": [
    "muffles <molefas@gmail.com>",
    "mstrk <mstrk512@gmail.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Cloudoki/react-redux-donderstarter/issues"
  },
  "homepage": "https://github.com/Cloudoki/react-redux-donderstarter.git#readme",
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "history": "4.7.2",
    "immutability-helper": "2.5.0",
    "js-cookie": "2.2.0",
    "prop-types": "15.6.0",
    "react": "16.1.1",
    "react-dom": "16.1.1",
    "react-redux": "5.0.6",
    "react-router": "4.2.0",
    "react-router-redux": "5.0.0-alpha.8",
    "redux": "3.7.2",
    "redux-saga": "0.16.0",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "autoprefixer": "8.2.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.2",
    "babel-jest": "22.4.3",
    "babel-loader": "7.1.4",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "babili-webpack-plugin": "0.1.2",
    "commitizen": "2.9.6",
    "cross-env": "5.1.4",
    "css-hot-loader": "1.3.9",
    "css-loader": "0.28.11",
    "cz-conventional-changelog": "2.1.0",
    "enzyme": "3.2.0",
    "enzyme-adapter-react-16": "1.1.0",
    "eslint": "4.11.0",
    "eslint-config-standard": "10.2.1",
    "eslint-config-standard-react": "5.0.0",
    "eslint-import-resolver-webpack": "0.8.3",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-node": "5.2.1",
    "eslint-plugin-promise": "3.6.0",
    "eslint-plugin-react": "7.4.0",
    "eslint-plugin-standard": "3.0.1",
    "file-loader": "1.1.11",
    "html-webpack-plugin": "3.2.0",
    "http-server-spa": "1.2.0",
    "image-webpack-loader": "4.2.0",
    "inquirer": "5.2.0",
    "jest": "21.2.1",
    "json-server": "0.12.1",
    "mini-css-extract-plugin": "0.4.0",
    "node-sass": "4.8.3",
